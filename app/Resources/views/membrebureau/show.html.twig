{% extends 'base.html.twig' %}

{% block body %}
    <h1>MembreBureau</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ membreBureau.id }}</td>
            </tr>
            <tr>
                <th>Datedebut</th>
                <td>{% if membreBureau.dateDebut %}{{ membreBureau.dateDebut|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{% if membreBureau.dateFin %}{{ membreBureau.dateFin|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ membreBureau.status }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('membrebureau_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('membrebureau_edit', { 'id': membreBureau.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
