{% extends "::base.html.twig" %}

{% block title %}backBundle:Info:index{% endblock %}

{% block body %}
<h1>Welcome to the Info:index page</h1>
    
    {% for info in infos %}
        
        <p>
            {{ info.titre }}<br/>
            {{ info.texte }}<br/>
            {{ info.date|date('Y-m-d') }}<br/>
            {{ info.type.libelle }}<br/>
        </p>
        
    {% endfor%}

{% endblock %}
